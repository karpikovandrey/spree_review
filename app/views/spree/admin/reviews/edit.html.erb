<% content_for :page_title do %>
    <%= link_to Spree.t(:reviews), admin_reviews_url %> /
    <%= @review.title %>
<% end %>

<%= form_for @review, url: spree.admin_review_url(@review), method: :put do |f| %>
    <div class="row">
      <div class="col-md-12">
        <%= render partial: 'form', locals: { f: f } %>
        <%= render partial: 'spree/admin/shared/edit_resource_links', locals: { collection_url: spree.admin_reviews_url } %>
      </div>
    </div>

<% end %>

